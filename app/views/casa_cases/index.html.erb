<p id="notice"><%= notice %></p>

<h1>Your Cases</h1>

<table>
  <thead>
    <tr>
      <th>Case number</th>
      <th>Teen program eligible</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @casa_cases.each do |casa_case| %>
      <tr>
        <td><%= casa_case.case_number %></td>
        <td><%= casa_case.teen_program_eligible %></td>
        <td><%= link_to 'Show', casa_case %></td>
        <td>
          <%= link_to new_case_contact_path(casa_case_id: casa_case.id) do %>
            <i class="far fa-calendar-plus"></i>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
