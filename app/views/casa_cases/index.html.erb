<p id="notice"><%= notice %></p>

<h1>Your Cases</h1>

<table>
  <thead>
    <tr>
      <th>Case number</th>
      <th>Teen program eligible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @casa_cases.each do |casa_case| %>
      <tr>
        <td><%= casa_case.case_number %></td>
        <td><%= casa_case.teen_program_eligible %></td>
        <td><%= link_to 'Show', casa_case %></td>
        <td><%= link_to 'Edit', edit_casa_case_path(casa_case) %></td>
        <td><%= link_to 'Destroy', casa_case, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <i class="far fa-calendar-plus"></i>
      </tr>
    <% end %>
  </tbody>
</table>
